<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1><%= title %></h1>
    <p>Welcome to <%= title %></p>
    <a href="/new">Add a new message!</a>
    <h2>Messages</h2>
    <% messages.forEach(function(message) { %>
      <h3><%= message.user %></h3>
      <ul>
        <li><%= message.text %></li>
        <li><%= message.added %></li>
      </ul>
    <% }); %>
  </body>
</html>




